{
  presets: [
    '@babel/preset-env'
  ],
  plugins: [
    'add-module-exports',
    'syntax-dynamic-import',
    'transform-object-assign',
    ["module-resolver", {
      "root": [
        "./docs/_client/src/**",
        "./docs/_client/src/js/**",
